{
    "_version": "1.0.0",
    "sap.app": {
        "id": "sap-app",
        "type": "application",
        "title": "Sap tachyon",
        "description": "Sap app of Tachyon",
        "applicationVersion": {
            "version": "1.0.0"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "rootView": {
            "viewName": "sap-app.views.index",
            "type": "XML",
            "async": true,
            "id": "sap-app"
        },
        "dependencies": {
            "minUI5Version": "1.98.0",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {}
            }
        },
        "models": {
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "type": "View",
                "viewType": "XML",
                "path": "sap-app.views",
                "controlId": "sap-app",
                "controlAggregation": "pages",
                "transition": "slide",
                "async": true
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "home",
                    "target": "home"
                },
                {
                    "pattern": "list-items",
                    "name": "listItems",
                    "target": "listItems"
                }
            ],
            "targets": {
                "home": {
                    "id": "home",
                    "name": "home",
                    "level": 1
                },
                "listItems": {
                    "id": "listItems",
                    "name": "list-items",
                    "level": 2
                }
            }
        }
    }
}